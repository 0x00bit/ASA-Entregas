zone "asa.br" {
    type master;
    file "/etc/bind/db.asa.br";
    allow-query { any; };
};

zone "15.168.192.in-addr.arpa" {
    type master;
    file "/etc/bind/db.192.168.15";
    allow-query { any; };
};

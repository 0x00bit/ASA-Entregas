$TTL    604800
@       IN      SOA     ns.asa.br. admin.asa.br. (
                  3     ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800 )   ; Negative Cache TTL
;
@       IN      NS      ns.asa.br.
@       IN      A       192.168.15.100  ; IP do proxy-server na rede do host
ns      IN      A       192.168.15.3    ; IP do dns-server na rede do host

; Servi√ßos na rede interna (containers)
web     IN      A       172.21.0.2      ; IP do web container
web02   IN      A       172.21.0.3      ; IP do web02 container

; Registros para os virtual hosts do proxy
www     IN      CNAME   asa.br.
julio   IN      A       192.168.15.100  ; Aponta para o proxy
magnus  IN      A       192.168.15.100  ; Aponta para o proxy
;
; BIND data file for local loopback interface
;
$ORIGIN asa.br.
$TTL	604800
@	IN	SOA	ns.asa.br.      admin.asa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@		IN	NS	ns.asa.br.
@		IN	A	192.168.15.6

ns1     IN  A   192.168.15.6  ; Registro do nameserver
www     IN  A   192.168.15.6  ; IP do servidor web (se aplic√°vel)
